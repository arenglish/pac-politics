{
  "name": "ng-workspace",
  "version": "0.0.0",
  "private": true,
  "lint-staged": {
    "*.{js,ts,json,scss,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "ng build",
    "process-photos": "node build/load-photos",
    "compress": "node build/compress.js",
    "cz": "./node_modules/.bin/git-cz",
    "dep-pac": "firebase use pac-politics && firebase deploy --public dist/pac-politics",
    "deploy-port": "firebase use portfolio && firebase deploy --public dist/portfolio",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:ts": "eslint --ext=js,ts --fix",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged",
        "sort-package-json"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/fire": "^5.2.3",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "camelcase-keys": "^5.2.0",
    "class-transformer": "^0.2.3",
    "core-js": "^2.6.10",
    "firebase": "^5.11.1",
    "firebaseui": "^4.4.0",
    "lodash": "^4.17.21",
    "logrocket": "^0.6.21",
    "ngx-markdown": "^8.2.1",
    "ngx-take-until-destroy": "^5.4.0",
    "request": "^2.88.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.2.0",
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular-devkit/build-ng-packagr": "^0.803.19",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.10.59",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "brotli": "^1.3.2",
    "codelyzer": "~4.5.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "firebase-tools": "^7.7.0",
    "husky": "^3.0.9",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^9.4.3",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "sort-package-json": "^1.23.1",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.10.0",
    "typescript": "~3.5.3"
  }
}
